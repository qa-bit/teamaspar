<div class="homegallery col-xs-12 col-md-6">
    <div class="homegallery-inner">
        <div id="swiper-gallery-{{ g.id }}" class="swiper-container" style="width:100%; height : 14em">
            <div class="swiper-wrapper">
                {% for m in g.medias %}
                    <div
                            href="{{ m.url ? m.url : '#' }}"
                            class="swiper-slide dark lazy cover"
                            data-src="{% path m, 'xlarge' %}"
                            aria-role="img"
                            title="{{ m.name }}"
                            arial-label="{{ m.name }}"
                    >
                    </div>
                {% endfor %}
            </div>
            <div id="swp-gallery-{{ g.id }}" class="swiper-pagination"></div>
        </div>
    </div>
</div>