<div class="homesponsors">
    <img class="sponsor1 tcenter img img-responsive" src="{{ asset('/bundles/motogp/img/assets/sponsor1.png') }}" alt="" />

    <div class="col-xs-12">
        <div class="col-xs-12 col-md-6 sponsor-logo color">
            {% for s in colorSponsors %}
                <a target="_blank" href="{{ s.webUrl }}">
                    <img src="{% path s.featuredMedia, 'big' %}" />
                </a>
            {% endfor %}
        </div>
        <div class="col-xs-12 col-md-6 sponsor-logo bn">
            {% for s in bnSponsors %}
                <a target="_blank" href="{{ s.webUrl }}">
                    <img src="{% path s.featuredMedia, 'big' %}" alt="" />
                </a>
            {% endfor %}
        </div>
    </div>

    <div class="text-center">
        <img src="{{ asset('/bundles/motogp/img/assets/up.png') }}" alt="" class="up-arrow">
    </div>

    <div class="col-xs-12">
        <img class="tcenter social-bottom" src="{{ asset('/bundles/motogp/img/assets/social_bottom.png') }}" alt="" />
    </div>
</div>