<div class="motos-gallery">
    {% for m in g %}
        <div class="motos-gallery-item">
            <div class="relative" style="background:url('{% path m, 'big' %}');height:14em">
                <a class="fancybox" rel="motos" href="{% path m, 'xlarge' %}" title="{{ m.description }}">
                    <img src="{% path m, 'big' %}" alt="" />
                </a>
            </div>
        </div>
    {% endfor %}
</div>