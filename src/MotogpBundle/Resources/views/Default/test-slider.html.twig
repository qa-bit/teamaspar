<section id="slider-inicio" class="clearfix">
    <div id="swiper-inicio" class="swiper-container" style="width:100%; height : 33em">
        <div class="swiper-wrapper">
            {% for m in gallery.medias %}
                    <div
                            class="swiper-slide dark lazy contain"
                            data-src="{% path m, 'xlarge' %}"
                            aria-role="img"
                            title="{{ m.name }}"
                            arial-label="{{ m.name }}"
                    >
                    </div>
            {% endfor %}
        </div>
    </div>
    <div class="paginator">
        <div id="swiper-celebraciones-pagination" class="swiper-pagination"></div>
    </div>
</section>
